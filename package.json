		"postcss": "npx postcss src/main.css -o resources/public/tailwind.css --verbose"
	},
	"dependencies": {
		"autoprefixer": "^10.4.19",
		"flowbite-react": "^0.7.3",
		"graceful-fs": "^4.2.11",
		"postcss": "^8.4.38",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-icons": "^5.0.1",
		"tailwindcss": "^3.4.1"
	},
	"devDependencies": {
		"shadow-cljs": "2.26.2"
	}
}
